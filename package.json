{
  "name": "svelte3-rollup-typescript-eslint-vscode",
  "version": "1.2.0",
  "description": "starter for svelte 3/rollup/typescript/eslint/vscode",
  "keywords": [
    "svelte",
    "rollup",
    "typescript",
    "eslint"
  ],
  "main": "src/index.ts",
  "license": "MIT",
  "author": "Tony Rewin",
  "scripts": {
    "cleanup": "rimraf ./public/bundle.*",
    "test": "cross-env NODE_ENV=test jest",
    "start": "npm run cleanup && cross-env NODE_ENV=development rollup -c -w",
    "build": "npm run cleanup && cross-env NODE_ENV=production rollup --config",
    "dev": "npm run start",
    "lint": "eslint ./src/**/*"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@pyoner/svelte-ts-preprocess": "^1.2.0",
    "@pyoner/svelte-types": "^3.4.4-2",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "canary",
    "babel-jest": "^24.8.0",
    "chokidar": "^3.0.1",
    "cross-env": "^6.0.0",
    "cssnano": "^4.1.10",
    "deasync": "^0.1.15",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^24.8.0",
    "postcss": "^7.0.18",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint": "9.0.0",
    "prettier-plugin-svelte": "0.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "0.22.1",
    "svelte": "^3.5.3",
    "svelte-preprocess": "^3.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
